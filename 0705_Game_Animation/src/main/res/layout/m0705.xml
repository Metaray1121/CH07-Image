<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/m0705_r001"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back06"
    android:padding="@dimen/p20"
    android:clipChildren="false"
    tools:context=".M0705">

    <TextView
        android:id="@+id/m0705_t001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:background="@color/Yellow"
        android:gravity="center"
        android:text="@string/m0705_t001"
        android:textColor="@color/colorAccent"
        android:textSize="@dimen/fontsize" />

    <TextView
        android:id="@+id/m0705_t002"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_t001"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/Aqua"
        android:gravity="center"
        android:text="@string/m0705_t002"
        android:textSize="@dimen/fontsize" />

    <TextView
        android:id="@+id/m0705_t003"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/m0705_t002"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="30dp"
        android:background="@color/Aqua"
        android:gravity="center"
        android:text="@string/m0705_t003"
        android:textSize="@dimen/fontsize" />

    <ImageButton
        android:id="@+id/m0705_b001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_t003"
        android:layout_alignEnd="@+id/m0705_t003"
        android:layout_marginTop="@dimen/p10"
        android:layout_marginEnd="@dimen/p10"
        android:background="@drawable/circle_shape"
        android:src="@drawable/scissors"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/m0705_b002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_b001"
        android:layout_alignEnd="@+id/m0705_b001"
        android:layout_marginTop="@dimen/p10"
        android:background="@drawable/circle_shape"
        android:src="@drawable/stone"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/m0705_b003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_b002"
        android:layout_alignEnd="@+id/m0705_b002"
        android:layout_marginTop="@dimen/p10"
        android:background="@drawable/circle_shape"
        android:src="@drawable/net"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <ImageSwitcher
        android:id="@+id/m0705_c001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/m0705_t002"
        android:layout_alignTop="@+id/m0705_b002"
        android:layout_marginStart="@dimen/p10"
        android:layout_marginTop="0dp"
        android:background="@drawable/ring"
        app:srcCompat="@drawable/scissors" />

    <TextView
        android:id="@+id/m0705_s001"
        android:layout_width="395dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_b003"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/Margins"
        android:layout_marginTop="@dimen/Margins"
        android:layout_marginEnd="@dimen/Margins"
        android:layout_marginBottom="@dimen/Margins"
        android:background="@color/Gray"
        android:text="@string/m0705_s001"
        android:textColor="@color/Red"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/m0705_f000"
        android:layout_width="395dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0705_s001"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/Margins"
        android:layout_marginEnd="@dimen/Margins"
        android:layout_marginBottom="@dimen/Margins"
        android:background="@color/White"
        android:text="@string/m0705_f000"
        android:textColor="@color/Gray"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:text="@string/myname"
        android:textColor="@color/White"
        android:textSize="@dimen/fontsize" />
</RelativeLayout>