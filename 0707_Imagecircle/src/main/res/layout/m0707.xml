<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Silver"
    tools:context=".M0707">

    <TextView
        android:id="@+id/m0707_t001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="107dp"
        android:layout_marginTop="233dp"
        android:text="@string/m0707_t001"
        android:textColor="@color/Navy"
        android:textSize="@dimen/fontsize" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:gravity="end"
        android:text="@string/myname"
        android:textColor="@color/Navy"
        android:textSize="@dimen/fontsize" />

    <tw.tcnr12.m0707.CircleImgView
        android:id="@+id/m0707_imgview"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="159dp"
        android:layout_marginTop="140dp"
        android:background="#00000000"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/a01" />

    <tw.tcnr12.m0707.CircleImgbutton
        android:id="@+id/m0707_imgbutton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="264dp"
        android:layout_marginTop="87dp"
        android:background="#00000000"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/img02th"
        tools:ignore="SpeakableTextPresentCheck" />
</RelativeLayout>